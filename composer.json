{
	"name": "devrun/framework",
	"type": "library",
	"minimum-stability": "dev",
	"description": "Simple and easy for use system for managing your websites.",
	"keywords": ["nette", "framework"],
	"license": "proprietary",
	"authors": [
		{
			"name": "Pavel Paulik"
		}
	],
	"require": {
		"php": "^7.3",
		"nette/nette": "^v3.0@dev",
		"kdyby/events": "^3.2@dev",
		"kdyby/fake-session": "^3.0",
		"kdyby/translation": "^3.0@dev",
		"contributte/image-storage": "^1.2@dev",
		"devrun/monolog": "^1.3@dev",

		"ext-gd": "*",
		"ext-curl": "*",
		"ext-sqlite3": "*",
		"ext-json": "*",
		"ext-pdo": "*",
		"ext-simplexml": "*",
		"ext-iconv": "*",
		"ext-fileinfo": "*",
		"ext-zend-opcache": "*",
		"ext-openssl": "*"
	},
	"require-dev": {
		"contributte/codeception": "^1.4@dev"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/developer-run/Monolog"
		}
	],
	"suggest": {
		"kdyby/monolog": "Allow log to syslog service. This is conflict with tracy/tracy or something."
	},
	"autoload": {
		"classmap": ["Devrun/"]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.9-dev"
		}
	}
}
